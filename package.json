{
  "name": "content-repository",
  "private": true,
  "description": "Using gatsby / netlify / github as a contant repository and seperate site data",
  "version": "0.1.0",
  "author": "Nathan Mackenzie",
  "dependencies": {
    "gatsby": "^2.10.4",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-source-filesystem": "^2.1.0",
    "gatsby-transformer-json": "^2.2.0",
